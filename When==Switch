fun main(args: Array<String>) { 
    val x:Int=5 
    when (x) { 
    1 -> print("x == 1") 
    2 -> print("x == 2") 
    else -> { // Note the block 
        println("x is neither 1 nor 2") 
    } 
} 


   val letter: Char='A' // defining a variable  

   
    
    when(letter){
        'A' -> println("A") 

    }
}
